<% @title = 'Acconut Info Edit' %>
<header class="container-fluid">
  <div class="row">
    <div class="col-md-2"></div>
      <div class="col-md-8">
         <h1><%= @title %></h1>
      </div>
    <div class="col-md-2"></div>
  </div>
</header>

<div class="row">
   <div class="col-md-2"></div>
   <div class="col-md-8">
       <div class="table-responsive">
         <%= form_for @staff_member, url: :staff_account do |f| %>
           <%= render 'form', f: f %>
             <div class="form-group">
                <div class="col-md-3">
                  <%= f.submit 'Submit' %>
                  <%= link_to 'Cansel', :staff_account %>
                </div>
             </div>
         <% end %>
       </div>
   </div>
  <div class="col-md-2"></div>
</div>
