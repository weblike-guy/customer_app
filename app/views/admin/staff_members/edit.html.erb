<% @title = 'Staff Info Edit' %>
 <h1 class="maintitle"><%= @title %></h1>
<div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-8 generic-form">
     <%= form_for [ :admin, @staff_member ] do |f|  %>
       <%= render 'form', f: f %>
        <div class="form-group">
           <div class="col-sm-3"><%= f.submit 'Update' %>
           <%= link_to 'cansel', :admin_staff_members %>
            </div>
        </div>
      <% end %>
    </div>
    <div class="col-md-2"></div>
</div>

